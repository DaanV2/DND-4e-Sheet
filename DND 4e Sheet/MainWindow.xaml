<Window x:Name="window" x:Class="DND_4e_Sheet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DND_4e_Sheet"
        xmlns:local1="clr-namespace:DaanV2.DND"
        mc:Ignorable="d"
        Title="DND 4e Sheet" Height="450" Width="800">
    <Window.DataContext>
        <local1:MainWindowViewModel/>
    </Window.DataContext>
    <Grid>
        <Frame x:Name="MainFrame"
            Content="{Binding DataContext.Current, ElementName=window, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"  Grid.Row="1" NavigationUIVisibility="Hidden" />
    </Grid>
</Window>
